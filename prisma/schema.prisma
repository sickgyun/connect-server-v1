generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

enum Role {
  STUDENT
  GRADUATE
}

model User {
  id          Int     @id
  name        String
  email       String
  profile_url String
  cardinal    Int
  role        Role    @default(STUDENT)
  github_id   String?
  company     String?
  isGraduate  Boolean
}
