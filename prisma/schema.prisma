generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

enum Role {
  STUDENT
  GRADUATE
}

model User {
  id          Int     @id
  github_id   String?
  profile_url String
  name        String
  semester    Int
  role        Role    @default(STUDENT)
}
